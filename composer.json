{
  "name": "medlib-v2/medlib",
  "description": "A librarian application for researchers and students",
  "keywords": ["library", "book", "search"],
  "license": "MIT",
  "type": "project",
  "require": {
    "php": ">=5.6.4",
    "alaouy/youtube": "^1.2",
    "andywer/js-localization": "dev-laravel-5",
    "backup-manager/laravel": "^1.1",
    "danmichaelo/quitesimplexmlelement": "^0.4.2",
    "greggilbert/recaptcha": "^2.1",
    "guzzlehttp/guzzle": "^6.2",
    "infyomlabs/laravel-generator": "dev-master",
    "intervention/image": "^2.3",
    "james-heinrich/getid3": "^1.9",
    "laravel-notification-channels/webpush": "^0.2.0",
    "laravel/framework": "5.4.*",
    "laravel/socialite": "^3.0",
    "laravel/tinker": "~1.0",
    "laravelcollective/html": "^5.4",
    "mccool/laravel-auto-presenter": "^5.0",
    "medlib/cover": "^1.0",
    "medlib/helpers": "~1.0",
    "medlib/yaz": "^2.1",
    "nesbot/Carbon": "^1.22",
    "nothingworks/blade-svg": "^0.1.0",
    "predis/predis": "~1.1",
    "prettus/l5-repository": "^2.6",
    "pusher/pusher-php-server": "^2.6",
    "symfony/process": "^3.2",
    "tymon/jwt-auth": "^0.5.10",
    "version/version": "^2.2",
    "wisembly/elephant.io": "^3.2",
    "zizaco/entrust": "^1.7"
  },
  "require-dev": {
    "barryvdh/laravel-debugbar": "^2.3",
    "barryvdh/laravel-ide-helper": "^2.2",
    "block8/php-docblock-checker": "^1.1",
    "codeclimate/php-test-reporter": "^0.4.2",
    "diablomedia/phpunit-pretty-printer": "^1.0",
    "doctrine/dbal": "^2.5",
    "filp/whoops": "^2.1",
    "friendsofphp/php-cs-fixer": "^2.0",
    "fzaninotto/faker": "~1.4",
    "jakub-onderka/php-console-highlighter": "^0.3.2",
    "jakub-onderka/php-parallel-lint": "^0.9.2",
    "laracasts/testdummy": "^2.3",
    "laravel/browser-kit-testing": "^1.0",
    "laravel/dusk": "^1.0",
    "mockery/mockery": "0.9.*",
    "phpmd/phpmd": "^2.6",
    "phpro/grumphp": "^0.11.1",
    "phpspec/phpspec": "^3.2",
    "phpunit/php-code-coverage": "^4.0",
    "phpunit/phpunit": "~5.7",
    "sebastian/exporter": "^2.0",
    "squizlabs/php_codesniffer": "^2.8",
    "symfony/css-selector": "^3.2",
    "symfony/dom-crawler": "^3.2",
    "themsaid/laravel-langman": "^1.3"
  },
  "autoload": {
    "classmap": [
      "database"
    ],
    "files": [
      "app/Helpers/Helpers.php"
    ],
    "psr-4": {
      "Medlib\\": "app/",
      "Tests\\": "tests/"
    }
  },
  "autoload-dev": {
    "classmap": [
        "tests/TestCase.php",
        "tests/BrowserKitTestCase.php",
        "tests/DuskTestCase.php"
    ]
  },
  "scripts": {
    "pre-install-cmd": [
      "php artisan clear-compiled"
    ],
    "post-root-package-install": [
      "php -r \"file_exists('.env') || copy('.env.example', '.env');\"",
      "([ -f .env ] && php artisan medlib:install) || exit 0"
    ],
    "post-create-project-cmd": [
      "php artisan key:generate"
    ],
    "post-install-cmd": [
      "Illuminate\\Foundation\\ComposerScripts::postInstall",
      "([ -f .env ] && php artisan optimize) || exit 0",
      "([ -f .env ] && php artisan js-localization:refresh) || exit 0",
      "php artisan cache:clear",
      "php artisan view:clear"
    ],
    "pre-update-cmd": [
      "php artisan clear-compiled"
    ],
    "post-update-cmd": [
      "Illuminate\\Foundation\\ComposerScripts::postUpdate",
      "([ -f .env ] && php artisan ide-helper:generate) || exit 0",
      "([ -f .env ] && php artisan ide-helper:meta) || exit 0",
      "php artisan optimize"
    ],
    "post-autoload-dump": [
      "([ -f .env ] && php artisan ide-helper:meta) || exit 0",
      "([ -f .env ] && php artisan optimize) || exit 0"
    ],
    "phpunit": "phpunit",
    "phpmd": "phpmd app/ text ./phpmd-rules.xml",
    "phpcs": "phpcs --standard=./phpcs-rules.xml --encoding=utf-8 app/ --colors -p -n",
    "phpcbf": "phpcbf --standard=./phpcs-rules.xml --encoding=utf-8 app/"
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true
  },
  "repositories": {
    "packagist": {
      "type": "composer",
      "url": "https://packagist.phpcomposer.com"
    }
  }
}
