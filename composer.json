{
    "name": "medlib-v2/medlib",
    "description": "A librarian application for researchers and students",
    "keywords": [
        "library",
        "book",
        "search"
    ],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=5.6.4",
        "laravel/framework": "5.3.16",
        "laravelcollective/bus": "5.3.*",
        "laravelcollective/html": "5.3.*",
        "medlib/helpers": "dev-master",
        "medlib/yaz": "dev-master",
        "medlib/marcxml": "dev-master",
        "danmichaelo/quitesimplexmlelement": "~0.2.4",
        "greggilbert/recaptcha": "^2.1",
        "nesbot/Carbon": "^1.21",
        "phpspec/phpspec": "^2.5",
        "sebastian/exporter": "~1.0",
        "intervention/image": "dev-master",
        "wisembly/elephant.io": "3.0.0",
        "james-heinrich/getid3": "^1.9",
        "barryvdh/laravel-ide-helper": "^2.1",
        "guzzlehttp/guzzle": "^6.1",
        "tymon/jwt-auth": "^0.5.6",
        "nothingworks/blade-svg": "^0.1.0"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "5.6.*",
        "laracasts/testdummy": "~2.0",
        "symfony/css-selector": "3.1.*",
        "symfony/dom-crawler": "3.1.*",
        "barryvdh/laravel-ide-helper": "~2.2",
        "barryvdh/laravel-debugbar": "~2.2"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "Medlib\\": "app/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "php artisan optimize"
        ],
        "pre-update-cmd": [
            "php artisan clear-compiled"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan optimize"
        ]
    },
    "config": {
        "preferred-install": "dist"
    },
	"minimum-stability": "dev"
}
