{
  "name": "Medlib",
  "version": "1.0.0",
  "main": "server.js",
  "description": "Medlib Gulp",
  "private": true,
  "scripts": {
    "postinstall": "cross-env NODE_ENV=production && gulp --production",
    "serve": "http-server -o -c 1 -a localhost",
    "prod": "gulp --production",
    "dev": "gulp watch",
    "pretest": "phplint 'app/**/*.php' 'database/**/*.php' 'tests/**/*.php' 'resources/views/**/*.php'",
    "test": "vendor/bin/phpcs app/ --standard=PSR2 --colors -p -n && vendor/bin/phpunit && karma start karma.config.js",
    "start": "supervisor --debug server.js"
  },
  "browserify": {
    "transform": [
      "babelify",
      "vueify"
    ]
  },
  "browser": {
    "vue": "vue/dist/vue.common.js"
  },
  "dependencies": {
    "dotenv": "^2.0.0",
    "ioredis": "^2.4.0",
    "jsonwebtoken": "^7.0.0",
    "parseuri": "^0.0.5",
    "phplint": "^1.7.1",
    "socket.io": "^1.4.5",
    "winston": "^2.3.0"
  },
  "devDependencies": {
    "babel-core": "^6.14.0",
    "babel-eslint": "^7.0.0",
    "babel-plugin-lodash": "^2.2.1",
    "babel-plugin-transform-runtime": "^6.3.13",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "^6.15.0",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-2": "^6.0.0",
    "babel-register": "^6.3.13",
    "babel-runtime": "^6.11.6",
    "babelify": "^7.3.0",
    "backbone": "^1.3.3",
    "backbone.localstorage": "^1.1.16",
    "blueimp-md5": "^2.4.0",
    "browserify": "^13.1.1",
    "browserify-hmr": "^0.3.5",
    "cross-env": "^3.1.3",
    "css-loader": "^0.26.1",
    "envify": "^4.0.0",
    "eslint": "^3.12.0",
    "eslint-config-standard": "^6.2.1",
    "eslint-plugin-html": "^1.7.0",
    "eslint-plugin-promise": "^3.4.0",
    "eslint-plugin-standard": "^2.0.1",
    "gulp": "^3.9.1",
    "gulp-shell": "^0.5.2",
    "html2js-browserify": "^1.1.0",
    "jasmine-core": "^2.5.2",
    "jquery": "^3.1.1",
    "karma": "^1.3.0",
    "karma-babel-preprocessor": "^6.0.1",
    "karma-browserify": "^5.1.0",
    "karma-jasmine": "^1.1.0",
    "karma-spec-reporter": "0.0.26",
    "karma-yandex-launcher": "^0.1.8",
    "laravel-elixir": "^6.0.0-15",
    "laravel-elixir-browserify-hmr": "^1.0.3",
    "laravel-elixir-browserify-official": "^0.1.3",
    "laravel-elixir-browsersync-official": "^1.0.0",
    "laravel-elixir-clean-unofficial": "^1.3.18",
    "laravel-elixir-vueify": "^2.0.0",
    "lodash": "^4.17.2",
    "postcss-cssnext": "^2.9.0",
    "proxyquireify": "^3.2.1",
    "supervisor": "^0.12.0",
    "vue": "^2.1.4",
    "vue-loader": "^10.0.2",
    "vue-style-loader": "^1.0.0",
    "vueify": "^9.4.0",
    "vueify-extract-css": "^0.2.0",
    "watchify": "^3.8.0"
  }
}
