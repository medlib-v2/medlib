{
  "name": "Medlib",
  "version": "1.0.0",
  "description": "Medlib Gulp",
  "private": true,
  "scripts": {
    "postinstall": "cross-env NODE_ENV=production && gulp --production",
    "prod": "cross-env NODE_ENV=production && gulp --production",
    "dev": "cross-env NODE_ENV=development && gulp watch",
    "pretest": "phplint 'app/**/*.php' 'database/**/*.php' 'tests/**/*.php' 'resources/views/**/*.php'",
    "test": "vendor/bin/phpcs app/ --standard=PSR2 --colors -p -n && vendor/bin/phpunit && karma start karma.config.js",
    "start": "supervisor --debug server.js"
  },
  "browserify": {
    "transform": [
      "babelify",
      "vueify"
    ]
  },
  "browser": {
    "vue": "vue/dist/vue.common.js"
  },
  "dependencies": {
    "backbone": "^1.3.3",
    "backbone.localstorage": "^1.1.16",
    "bootstrap-switch": "^3.3.2",
    "dotenv": "^2.0.0",
    "html-loader": "^0.4.4",
    "ioredis": "^2.4.0",
    "jquery": "^3.1.1",
    "jsonwebtoken": "^7.0.0",
    "laravel-elixir": "^6.0.0-15",
    "laravel-elixir-browserify-hmr": "^1.0.3",
    "laravel-elixir-browserify-official": "^0.1.3",
    "laravel-elixir-browsersync-official": "^1.0.0",
    "laravel-elixir-clean-unofficial": "^1.3.20",
    "laravel-elixir-vue-2": "^0.3.0",
    "laravel-elixir-webpack-official": "^1.0.10",
    "lodash": "^4.17.4",
    "parseuri": "^0.0.5",
    "socket.io": "^1.4.5",
    "socket.io-client": "^1.7.2",
    "vue": "^2.1.10",
    "vue-hot-reload-api": "^2.0.7",
    "vue-resource": "^1.0.3",
    "vuex": "^2.1.1",
    "winston": "^2.3.0"
  },
  "devDependencies": {
    "babel-core": "^6.21.0",
    "babel-eslint": "^7.1.1",
    "babel-plugin-lodash": "^3.2.11",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-stage-2": "^6.18.0",
    "babel-runtime": "^6.20.0",
    "babelify": "^7.3.0",
    "bootstrap-sass": "^3.3.7",
    "browserify": "^13.3.0",
    "browserify-hmr": "^0.3.5",
    "cross-env": "^3.1.4",
    "envify": "^4.0.0",
    "eslint": "^3.13.0",
    "eslint-config-standard": "^6.2.1",
    "eslint-plugin-html": "^1.7.0",
    "eslint-plugin-promise": "^3.4.0",
    "eslint-plugin-standard": "^2.0.1",
    "gulp": "^3.9.1",
    "gulp-shell": "^0.5.2",
    "jasmine-core": "^2.5.2",
    "karma": "^1.3.0",
    "karma-babel-preprocessor": "^6.0.1",
    "karma-browserify": "^5.1.0",
    "karma-jasmine": "^1.1.0",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-spec-reporter": "^0.0.26",
    "phantomjs-prebuilt": "^2.1.14",
    "phplint": "^1.7.1",
    "postcss-cssnext": "^2.9.0",
    "proxyquireify": "^3.2.1",
    "supervisor": "^0.12.0",
    "vueify": "^9.4.0",
    "watchify": "^3.8.0"
  }
}
